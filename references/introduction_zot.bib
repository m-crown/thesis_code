@article{Sigrist2002-af,
  title = {{{PROSITE}}: A Documented Database Using Patterns and Profiles as Motif Descriptors},
  author = {Sigrist, Christian J A and Cerutti, Lorenzo and Hulo, Nicolas and Gattiker, Alexandre and Falquet, Laurent and Pagni, Marco and Bairoch, Amos and Bucher, Philipp},
  date = {2002-09},
  journaltitle = {Briefings in Bioinformatics},
  shortjournal = {Brief. Bioinform.},
  volume = {3},
  number = {3},
  pages = {265--274},
  publisher = {{Oxford University Press (OUP)}},
  abstract = {Among the various databases dedicated to the identification of protein families and domains, PROSITE is the first one created and has continuously evolved since. PROSITE currently consists of a large collection of biologically meaningful motifs that are described as patterns or profiles, and linked to documentation briefly describing the protein family or domain they are designed to detect. The close relationship of PROSITE with the SWISS-PROT protein database allows the evaluation of the sensitivity and specificity of the PROSITE motifs and their periodic reviewing. In return, PROSITE is used to help annotate SWISS-PROT entries. The main characteristics and the techniques of family and domain identification used by PROSITE are reviewed in this paper.},
  langid = {english}
}

@article{Sillitoe2015-dx,
  title = {The History of the {{CATH}} Structural Classification of Protein Domains},
  author = {Sillitoe, Ian and Dawson, Natalie and Thornton, Janet and Orengo, Christine},
  date = {2015-12},
  journaltitle = {Biochimie},
  volume = {119},
  pages = {209--217},
  publisher = {{Elsevier BV}},
  abstract = {This article presents a historical review of the protein structure classification database CATH. Together with the SCOP database, CATH remains comprehensive and reasonably up-to-date with the now more than 100,000 protein structures in the PDB. We review the expansion of the CATH and SCOP resources to capture predicted domain structures in the genome sequence data and to provide information on the likely functions of proteins mediated by their constituent domains. The establishment of comprehensive function annotation resources has also meant that domain families can be functionally annotated allowing insights into functional divergence and evolution within protein families.},
  langid = {english},
  keywords = {Protein structure,Structure classification}
}

@article{Slatko2018-qe,
  title = {Overview of Next-Generation Sequencing Technologies: {{Overview}} of next-Generation Sequencing},
  author = {Slatko, Barton E and Gardner, Andrew F and Ausubel, Frederick M},
