@ARTICLE{Kuo2016-yb,
  title        = {Analyses of {{Coronavirus}} Assembly Interactions with
                  Interspecies Membrane and Nucleocapsid Protein Chimeras},
  author       = {Kuo, Lili and Hurst-Hess, Kelley R and Koetzner, Cheri A and
