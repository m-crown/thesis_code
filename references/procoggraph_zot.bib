@article{Teruel2023-ih,
  title = {Surfaces: A Software to Quantify and Visualize Interactions within and between Proteins and Ligands},
  author = {Teruel, Natália and Borges, Vinicius Magalhães and Najmanovich, Rafael},
  date = {2023-10},
