@article{Laskowski2005-np,
  title = {{{PDBsum}} More: New Summaries and Analyses of the Known {{3D}} Structures of Proteins and Nucleic Acids},
  author = {Laskowski, Roman A and Chistyakov, Victor V and Thornton, Janet M},
