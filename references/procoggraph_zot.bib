@article{Itoh2007-it,
  title = {Evolutionary History and Functional Implications of Protein Domains and Their Combinations in Eukaryotes},
  author = {Itoh, Masumi and Nacher, Jose C and Kuma, Kei-Ichi and Goto, Susumu and Kanehisa, Minoru},
  date = {2007},
